<h1>Listing contacts</h1>
    <h4>Total Clients: <%= @contacts.count %></h4>
<table>
  <thead>
    <tr>
      <th>Useme</th>
      <th>Coborrower</th>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Useemail</th>
      <th>Email</th>
      <th>Usephone</th>
      <th>Phone</th>
      <th>Other</th>
      <th>Record</th>
      <th>Profile</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.useme %></td>
        <td><%= contact.coborrower %></td>
        <td><%= contact.firstname %></td>
        <td><%= contact.lastname %></td>
        <td><%= contact.useemail %></td>
        <td><%= contact.email %></td>
        <td><%= contact.usephone %></td>
        <td><%= contact.phone %></td>
        <td><%= contact.other %></td>
        <td><%= contact.record_id %></td>
        <td><%= contact.profile_id %></td>
        <td><%= link_to 'Show', contact %></td>
        <td><%= link_to 'Edit', edit_contact_path(contact) %></td>
        <td><%= link_to 'Destroy', contact, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Contact', new_contact_path %>

<br>

<br>

<br>

<br>

<br>

<% if current_user.profile.title == "admin" || current_user.profile.title == "master" %>
  <h2>Backup</h2>
  <br />
  <div class="row">
    <div class="col-xs-4 col-sm-4">
    <h3>Export Contacts</h3>
      <%= link_to "To CSV File", contacts_path(format: "csv"), :class => 'btn btn-default btn' %>

    </div>
    <div class="col-xs-4 col-sm-4">
      <h3>Import Contacts</h3>
      <%= form_tag import_contacts_path, multipart: true do %>
        <%= file_field_tag :file, :class => 'btn btn-default btn' %><br />
        <%= submit_tag "Import", :class => 'btn btn-default btn' %>
      <% end %>
    </div>
  </div>
<% end %>
