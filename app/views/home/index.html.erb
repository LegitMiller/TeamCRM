	<%= render 'notifications/nlist' %>
	
  <div class="row">
    <div class="col-xs-3 col-sm-3">
  		<h2>My Current Clients</h2> 
    </div>
    <div class="col-xs-9 col-sm-9">
    	<div class="pull-right">
	  		<table class="table table-hover" id="records">
					<tbody>
					  <tr> Color Key:
					  	<td class=""><center>Updated less than 2 days ago</center></td>
					    <td class="success"><center>2 to 6 days ago</center></td>
					    <td class="warning"><center>6 to 10 days ago</center></td>
					    <td class="danger"><center>11 or more days ago</center></td>
					  </tr>
					</tbody>
				</table>
			</div>
	  </div>
	</div>


  <table class="table table-hover" id="records">
    <thead>
      <tr>
        <th>Frist Name</th>
        <th>Updated</th>
        <th>Progress</th>       
        <th>Loan Officer</th>
        <th>Processor</th>
        <th>Realtor</th>
      </tr>
    </thead>

    <tbody>
      <% if !@records.blank? %>
        <% @records.each do |record| %>
          <% if record.progress != "On Hold" and record.progress != "Completed" and record.progress != "Dropped" and record.progress != "Unknown" %>
           <!--<tr data-link="<%= edit_record_path(record) %>">-->
            <% myrecdate = getupdated(record) || nil %>
            <% if !myrecdate.blank? %>
              <% if myrecdate.to_date >= Date.today-2 %>
                <% @followupwithme = "" %> 
              <% elsif myrecdate.to_date >= Date.today-6 %>
                <% @followupwithme = "success" %> 
              <% elsif myrecdate.to_date >= Date.today-10 %>
                <% @followupwithme = "warning" %> 
              <% elsif myrecdate.to_date <= Date.today-11 %>
                <% @followupwithme = "danger" %> 
              <% else %>
                <% @followupwithme = "" %>
              <% end %>
            <% else %>
              <% @followupwithme = "" %> 
            <% end %>
            
            <%= content_tag :tr, class: "#{@followupwithme}" do -%> 

              <td><%= correctlink record, record.firstname, record.lastname %></td>
              <td>
                <%= time_ago_in_words(myrecdate) %>
              </td>

              <td><%= getpname record.detailedprogress %></td>
              <td><%= getname record.loanofficer_id %></td>
              <td><%= getname record.processor_id %></td>
              <td><%= getname record.real_id %></td>              
            <% end -%>
          <% end %>
        <% end %>
      <% else %>  
        <tr> 

          <td><center>No Active Records</center></td>

        </tr>
      <% end %>
    </tbody>
  </table>


<% if current_user.profile.title == "admin" or current_user.profile.title == "master" %>
