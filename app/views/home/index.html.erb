  <div class="row">
    <div class="col-xs-12 col-sm-12"><br />
      <div class="pull-right"><% if current_user.profile.title == "admin" or current_user.profile.title == "master" %>
        <%= link_to "Edit Progression Points", phases_path, class: 'btn btn-default btn-lg btn-warning', "data-no-turbolink"=>"true" %>    <% end %>
        <%= link_to 'New Record', new_record_path, class: 'btn btn-default btn-lg btn-info', "data-no-turbolink"=>"true"  %>
        <%= link_to "Client Search", records_path, class: 'btn btn-default btn-lg btn-warning', "data-no-turbolink"=>"true" %>
        <%= link_to "Profile Search", profiles_path, class: 'btn btn-default btn-lg btn-warning', "data-no-turbolink"=>"true" %>
      </div>
    </div>
</div>
<div class="clearfix visible-xs-block"></div>

<%= render 'notifications/nlist' %>
<br />

  <h3>My Current Clients</h3>
  <table class="table table-hover" id="records">
    <thead>
      <tr>
        <th>Frist Name</th>
        <th>Last Name</th>
        <th>Record Age</th>
        <th>Progress</th>       
        <th>Loan Officer</th>
        <th>Processor</th>
      </tr>
    </thead>

    <tbody>
      <% if !@records.blank? %>
        <% @records.each do |record| %>
          <% if record.progress != "On Hold" and record.progress != "Completed" and record.progress != "Dropped" and record.progress != "Unknown" %>
           <!--<tr data-link="<%= edit_record_path(record) %>">-->
            <tr> 
              <td><%= correctlink record, record.firstname %></td>
              <td><%= correctlink record, record.lastname %></td>
              <td><%= time_ago_in_words record.receivedate %></td>
              <td><%= getpname record.detailedprogress %></td>
              <td><%= getname record.loanofficer_id %></td>
              <td><%= getname record.processor_id %></td>
            </tr>
          <% end %>
        <% end %>
      <% else %>  
        <tr> 

          <td><center>No Active Records</center></td>

        </tr>
      <% end %>
    </tbody>
  </table>


<% if current_user.profile.title == "admin" or current_user.profile.title == "master" %>
  <br />
  <br />
  <h3>Admins</h3>
  <table class="table">

    <tbody>
      <% @profiles.each do |profile| %>
        <% if profile.title == "admin" %>
      <tr>
        <td>
          <%= getname profile.user_id%> 
        </td>
        <td>
          joined <%= time_ago_in_words profile.created_at %> ago
        </td>
      </tr>
      <% end %>
    <% end %> 
    </tbody>
  </table>  
  <!--<%= link_to "sup", "#{@mylink}" %>
  <%= link_to "sup2", @mylink %>-->
<% end %>